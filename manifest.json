{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 3,
	"name": "__MSG_name__",
	"version": "2.1",
	"default_locale": "en",
	"description": "__MSG_description__",
	"icons": {
		"16": "img/ico_16px.png",
		"48": "img/ico_48px.png",
		"128": "img/ico_128px.png"
	},
	"action": {
		"default_icon": {
			"19": "img/ico_19px.png",
			"38": "img/ico_38px.png"
		},
		"default_title": "__MSG_title__"
	},
	"author": "天天发蛋糕",
	"background": {
		"service_worker": "js/main.js"
	},
	"options_page": "/options.html",
	"options_ui": {
		"page": "/options.html"
	},
	"permissions": [
		"tabs",
		"storage"
	]
}